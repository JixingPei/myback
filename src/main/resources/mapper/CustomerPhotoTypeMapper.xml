<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.oldpei.myback.dao.CustomerPhotoTypeMapper">
    <resultMap id="BaseResultMap" type="com.oldpei.myback.model.CustomerPhotoType">
        <constructor>
            <idArg column="unique_id" jdbcType="INTEGER" javaType="java.lang.Integer"/>
            <arg column="type" jdbcType="VARCHAR" javaType="java.lang.String"/>
            <arg column="active_flg" jdbcType="CHAR" javaType="java.lang.String"/>
            <arg column="sys_insert_id" jdbcType="VARCHAR" javaType="java.lang.String"/>
            <arg column="sys_insert_date" jdbcType="TIMESTAMP" javaType="java.util.Date"/>
            <arg column="sys_update_id" jdbcType="VARCHAR" javaType="java.lang.String"/>
            <arg column="sys_update_date" jdbcType="TIMESTAMP" javaType="java.util.Date"/>
        </constructor>
    </resultMap>
    <insert id="addPhotoType">
        insert into myphoto.t_phototype (type, active_flg,
        sys_insert_id, sys_insert_date, sys_update_id,
        sys_update_date)
        values (#{type,jdbcType=VARCHAR}, #{activeFlg,jdbcType=CHAR},
        #{sysInsertId,jdbcType=VARCHAR}, #{sysInsertDate,jdbcType=TIMESTAMP}, #{sysUpdateId,jdbcType=VARCHAR},
        #{sysUpdateDate,jdbcType=TIMESTAMP})
    </insert>
    <select id="getAllPhotoType" resultType="com.oldpei.myback.model.CustomerPhotoType">
        select type from myphoto.t_phototype;
    </select>

</mapper>