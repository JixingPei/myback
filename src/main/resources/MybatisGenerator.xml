<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <!-- 本地硬盘数据库驱动包 -->
    <classPathEntry
            location="D:\develop\apache\apache-maven-3.6.1\repository-pei\mysql\mysql-connector-java\8.0.15\mysql-connector-java-8.0.15.jar"/>

    <context id="Mybatis3Context" targetRuntime="MyBatis3">
        <property name="javaFileEncoding" value="UTF-8"/>
        <commentGenerator>
            <!-- 去掉生成日期那行注释 -->
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成所有的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/personInfo?useLegacyDatetimeCode=false&amp;serverTimezone=UTC&amp;nullCatalogMeansCurrent=true&amp;useSSL=false"
                        userId="root"
                        password="123456">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- 模型的包名和位置-->
        <javaModelGenerator targetPackage="com.oldpei.personinfo.model"
                            targetProject="C:\Users\18247\Desktop\ws\personinfoBack\src\main\java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- 映射文件的包名和位置-->
        <sqlMapGenerator targetPackage="mapper"
                         targetProject="C:\Users\18247\Desktop\ws\personinfoBack\src\main\resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>

        <!-- DAO的包名和位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.oldpei.personinfo.dao"
                             targetProject="C:\Users\18247\Desktop\ws\personinfoBack\src\main\java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <table tableName="t_diary" domainObjectName="DiaryModel" enableCountByExample="false"
               enableDeleteByExample="false" enableUpdateByExample="false" enableSelectByExample="false">
        </table>
    </context>

</generatorConfiguration>